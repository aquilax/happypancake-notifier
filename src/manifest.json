{
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon_19.png"
  },
  "default_locale": "en",
  "description": "__MSG_hp_description__",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "name": "__MSG_hp_name__",
  "optional_permissions": [
    "tabs"
  ],
  "permissions": [
    "alarms",
    "*://*.happypancake.com/"
   ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
